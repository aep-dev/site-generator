---
import sideBar from "../../full_aeps.json";
import AepListEntry from "./AepListEntry.astro";
---

{
  sideBar.map((item) => (
    <div>
        <h1>{item.label}</h1>
            {item.items.map((aep) => (
                <AepListEntry title={aep.title} href={"/" + aep.slug} id={aep.id.toString()} />
            ))}
    </div>
  ))
}
